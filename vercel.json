{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/upload",
      "dest": "/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
